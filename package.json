{
  "name": "phoenix-project-site",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_DISABLE_PATCH_INCORRECT_LOCKFILE=1 next dev",
    "admin": "concurrently -k -n admin,site \"decap-server\" \"cross-env NEXT_DISABLE_PATCH_INCORRECT_LOCKFILE=1 next dev\"",
    "prebuild": "node scripts/generate-heroes-index.js && node scripts/generate-sitemap.js",
    "build": "cross-env NEXT_DISABLE_PATCH_INCORRECT_LOCKFILE=1 next build",
    "export": "cross-env NEXT_DISABLE_PATCH_INCORRECT_LOCKFILE=1 next export",
    "deploy": "node scripts/deploy.js",
    "lint": "next lint"
  },
  "dependencies": {
    "basic-ftp": "^5.0.5",
    "concurrently": "^8.2.2",
    "decap-cms": "^3.0.0",
    "decap-server": "^1.7.0",
    "dotenv": "^16.4.5",
    "gray-matter": "^4.0.3",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tailwindcss/line-clamp": "^0.4.4"
  }
}
